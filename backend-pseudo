$date1 = new DateTime('2023-08-01');
$date2 = new DateTime('2023-08-15');

$interval = $date1->diff($date2);

echo "Difference: ";
echo $interval->y . " years, ";
echo $interval->m . " months, ";
echo $interval->d . " days, ";
echo $interval->h . " hours, ";
echo $interval->i . " minutes, ";
echo $interval->s . " seconds";

time = interval->h

if interval->i || interval->s > 0 = time + 1

if interval->d > 0 = time + (interval->d*24)

cost  = 3000 * time
